{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "prettier"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true
    }
  },  
  "env": {
    "amd": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "jsx-quotes": [
      1,
      "prefer-double"
    ],
    "no-console": 0
  },
  "plugins": [
    "prettier"
  ]
}